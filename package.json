{
  "name": "reachout",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint ./server",
    "pretest": "NODE_ENV=test yarn db:migrate",
    "test": "jest --no-cache --detectOpenHandles --runInBand --forceExit",
    "build-server": "babel src --out-dir dist -s",
    "clean": "rm -rf dist && mkdir dist",
    "build": "yarn run clean && yarn run build-server",
    "serve": "node dist/index.js",
    "start": "NODE_ENV=production yarn run build && yarn run serve",
    "start:dev": "nodemon  app.js --exec babel-node",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate",
    "db:rollback": "./node_modules/.bin/sequelize db:migrate:undo:all"
  },
  "repository": "https://github.com/Mrshinezee/reachOut-backend.git",
  "author": "Mrshinezee <ijeomaharinze@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "apollo-server": "^2.4.8",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcrypt": "^3.0.5",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "express": "^4.16.4",
    "graphql": "^14.1.1",
    "jest": "^24.5.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "nodemon": "^1.18.10",
    "pg": "^7.9.0",
    "sequelize": "^5.1.0",
    "sequelize-cli": "^5.4.0",
    "supertest": "^4.0.2",
    "validator": "^10.11.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.3.0",
    "eslint-plugin-import": "^2.14.0"
  }
}
